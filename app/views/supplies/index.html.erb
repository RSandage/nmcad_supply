<h1>Supplies listing pages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @supplies.each do |supply| %>
    <tr>
      <td><%= supply.name %></td>
      <td><%= supply.amount %></td>
      <%# actions will go here %>
    </tr>
    <% end %>
  </tbody>
</table>